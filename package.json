{
  "name": "prequalifier",
  "version": "1.4.0",
  "main": "index.js",
  "repository": "https://github.com/wingedearth/prequalifier.git",
  "author": "Andrew A. Anissi <andrew@wingedearth.com>",
  "license": "MIT",
  "scripts": {
    "build:client": "node ./node_modules/webpack/bin/webpack --config webpack.client.config.js",
    "build:client:watch": "node ./node_modules/webpack/bin/webpack --config webpack.client.config.js --watch",
    "build:server": "node ./node_modules/webpack/bin/webpack --config webpack.server.config.js",
    "build:server:watch": "node ./node_modules/webpack/bin/webpack --config webpack.server.config.js --watch",
    "build:dev": "yarn run build:client:watch & yarn run build:server:watch",
    "build": "yarn clean && yarn build:client && yarn build:server",
    "clean": "rimraf dist",
    "dev": "nodemon ./dist/server/server.js",
    "prebuild:dev": "yarn clean && yarn build:client",
    "release": "standard-version",
    "start": "node ./dist/server/server.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.1.0",
    "husky": "2.4",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "purgecss-webpack-plugin": "^2.1.0",
    "standard-version": "^7.1.0",
    "style-loader": "^1.1.3",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "body-parser": "^1.19.0",
    "bulma": "^0.8.0",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "express": "^4.17.3",
    "formik": "^2.1.4",
    "glob": "^7.1.6",
    "node-sass": "^4.13.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "serialize-javascript": "^3.0.0",
    "webpack": "^4.41.6",
    "yup": "^0.28.1"
  }
